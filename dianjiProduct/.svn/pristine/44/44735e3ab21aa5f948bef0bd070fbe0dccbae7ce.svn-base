#head {
    height: auto;
    .mui-btn{
        color: white;
        font-size: 17px;
    }
    .mui-input-clear {
        background: white;
    }
    .mui-input-row {
        width: 85%;
        float: left;
    }
    .saomaImg {
        float: right;
        width: 7%;
        margin-top: 8px;
    }
}

#footer {
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    align-items: center;
    .mui-bar-tab-item {
        width: 18%;
        height: 60%;
        background: #524598;
        color: white;
        font-size: 12px;
        display: flex;
        justify-content: center;
        align-items: center;
    }
}

#content {
    padding-top: 85px;
    .mui-media-body {
        white-space: normal;
        .mui-ellipsis {
            white-space: normal;
            overflow: visible;
            .items{
                margin-right: 10px;
            }
        }
        .mui-progressbar{
            width: 40%;
            float: right;
            right: 18px;
            top: 5px;
            span{
//              background: #6db83a;
            }
        }
    }
}