* {
    padding: 0;
    margin: 0;
}

body {
    #head {
        background: #524598;
        .mui-title,
        a {
            color: white;
        }
    }
    #list {
        li {
            a {
                padding-right: 20px;
                padding-left: 15px;
                img {
                    width: 20px;
                    height: 20px;
                    float: left;
                    padding-right: 0px;
                    margin-right: 5px;
                }
                span {
                    float: right;
                }
            }
        }
    }
    .bottomButton {
        background: #524598;
        border-radius: 80px;
        width: 80%;
        margin: 0 auto;
        margin-top: 150px;
        margin-bottom: 50px;
    }
    
    

    #footer {
        display: flex;
        flex-direction: row;
        align-items: center;
//      border-top: 0;
//      box-shadow: none;
        button {
            align-self: center;
            background: #524598;
            border-radius: 80px;
            width: 60%;
            margin: 0 auto;
            display: block;
            margin-bottom: 10px;
        }
    }
}